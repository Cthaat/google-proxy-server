{
  "name": "google-maps-proxy-server",
  "version": "1.0.0",
  "description": "本地Google Maps API代理服务器，用于微信小程序",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "docker:build": "docker build -t google-maps-proxy:yarn .",
    "docker:build:prod": "docker build -t google-maps-proxy:prod-yarn -f Dockerfile.prod .",
    "docker:run": "docker run -p 3001:3001 google-maps-proxy:yarn",
    "docker:compose": "docker-compose up -d",
    "docker:clean": "docker system prune -f"
  },
  "keywords": [
    "google-maps",
    "proxy",
    "wechat-miniprogram",
    "api-proxy"
  ],
  "author": "高级中国全栈工程师",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "axios": "^1.6.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}